{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project":"./tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["off","always"],
        "@typescript-eslint/semi": "off",
        "no-unexpected-multiline": "error"
    }
}
