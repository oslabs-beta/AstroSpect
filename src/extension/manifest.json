{
  "manifest_version": 3,
  "name": "Astro Dev Tools Experiment",
  "version": "0.0.1",
  "devtools_page": "devtools.html",
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
