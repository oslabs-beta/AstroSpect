{
  "manifest_version": 3,
  "name": "AstroSpect",
  "version": "0.0.1",
  "icons": {
    "32": "assets/astrospect-logo.png",
    "16": "assets/astrospect-logo.png",
    "48": "assets/astrospect-logo.png",
    "128": "assets/astrospect-logo.png"
  },
  "devtools_page": "devtools.html",
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs"],

  "background": {
    "service_worker": "background.js"
  }
}
